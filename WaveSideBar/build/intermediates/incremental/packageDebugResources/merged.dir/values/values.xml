<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SearchEditText">
        <attr format="reference" name="drawableDel"/>
    </declare-styleable>
    <declare-styleable name="WaveSideBarView">
        <attr format="boolean" name="sidebar_lazy_respond"/>
        <attr format="color" name="sidebar_text_color"/>
        <attr format="dimension" name="sidebar_max_offset"/>
        <attr format="enum" name="sidebar_position">
            <enum name="right" value="0"/>
            <enum name="left" value="1"/>
        </attr>
        <attr format="enum" name="sidebar_text_alignment">
            <enum name="center" value="0"/>
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
        </attr>
    </declare-styleable>
    <string name="app_name">WaveSideBar</string>
</resources>